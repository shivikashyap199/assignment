    "def print_histogram(mp):\n",
    "    for i,j in mp.items():\n",
    "        print(f\"{i} | {'='*j} {j}\")\n",
    "\n",
    "n=int(input(\"enter the number of key-value pair\"))\n",
    "mp={}\n",
    "for i in range(n):\n",
    "    ele=int(input(\"enter the key\"))\n",
    "    value=int(input(\"enter the frequency\"))\n",
    "    mp[ele]=value\n",
    "print_histogram(mp)\n"
   ]
